{
  "expo": {
    "name": "Moto Manager",
    "slug": "moto-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "motomanager",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.c4zin.motomanager"
    },
    "android": {
      "package": "com.c4zin.motomanager",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "431453ff-45ef-4ed7-8c62-796505f02971"
      }
    }
  }
}
